import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    double[] num = new double[10]; 
		System.out.println("Escreva 10 n√∫meros");
		for (int i = 0; i < 10; i++) {
		    num[i] = sc.nextDouble();
		}
		for (int l = 9; l >= 0; l--) {
		    if (l == 0) {
		        System.out.print(num[l]);
		    } else 
		    System.out.print(num[l] + ", ");
		}
	sc.close();
	}
}
