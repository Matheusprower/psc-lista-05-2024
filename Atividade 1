import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int[] num = new int[5]; 
		System.out.println("Escreva 5 n√∫meros");
		for (int i = 0; i < 5; i++) {
		    num[i] = sc.nextInt();
		}
		for (int l = 0; l < 5; l++) {
		    if (l == 4) {
		        System.out.print(num[l]);
		    } else 
		    System.out.print(num[l] + ", ");
		}
	}
}
